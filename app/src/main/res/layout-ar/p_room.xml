<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/p_room_layout"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <View
        android:id="@+id/view28"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/Lightblack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include16" />

    <Button
        android:id="@+id/po_public_room"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:text="@string/tv_general_office"
        android:textColor="@color/white"
        app:layout_constraintEnd_toStartOf="@+id/view26"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <View
        android:id="@+id/view26"
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:background="@color/Lightblack"
        app:layout_constraintEnd_toStartOf="@+id/po_services"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/po_public_room"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/po_services"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:text="الخدمات"
        android:textColor="@color/white"
        app:layout_constraintEnd_toStartOf="@+id/view27"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view26"
        app:layout_constraintTop_toBottomOf="@+id/view28" />

    <View
        android:id="@+id/view27"
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:background="@color/Lightblack"
        app:layout_constraintEnd_toStartOf="@+id/po_direct"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/po_services"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/po_direct"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:text="التعميد المباشر"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view27"
        app:layout_constraintTop_toBottomOf="@+id/view28" />


    <View
        android:id="@+id/viewfr"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/Lightblack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/po_public_room" />

    <FrameLayout
        android:id="@+id/frame_p_room"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewfr">

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>