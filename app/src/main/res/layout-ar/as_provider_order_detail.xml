            <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/button_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout_reviews_stars">

                <Button
                    android:id="@+id/client_completeService"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="41dp"
                    android:background="@mipmap/bt_grey1"
                    android:text="تم انجاز المعاملة"
                    android:textColor="@color/white"
                    android:visibility="gone"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_accept_reject"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/client_completeService">

                    <Button
                        android:id="@+id/provider_accept"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="41dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="قبول"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toStartOf="@+id/provider_reject"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_goneMarginTop="5dp" />

                    <Button
                        android:id="@+id/provider_reject"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="41dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="رفض"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/provider_accept" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layoutCancelRequest"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/layout_accept_reject">

                    <TextView
                        android:id="@+id/cancelRequestText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="العميل يود الغاء طلب "
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />


                    <Button
                        android:id="@+id/btnCancelRequestAccept"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="قبول"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toStartOf="@+id/btnCancelRequestCancel"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cancelRequestText"
                        app:layout_goneMarginTop="5dp" />

                    <Button
                        android:id="@+id/btnCancelRequestCancel"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="10dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="رفض"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/btnCancelRequestAccept"
                        app:layout_constraintTop_toBottomOf="@id/cancelRequestText" />
                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_cancel_compplain"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/layoutCancelRequest">

                    <Button
                        android:id="@+id/provider_cancel"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="41dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="@string/cancel_order"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toStartOf="@+id/provider_complete"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toStartOf="parent" />

                    <Button
                        android:id="@+id/provider_complete"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="41dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="@string/complete_order"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/provider_cancel" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_warranty_payment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/layout_cancel_compplain">

                    <Button
                        android:id="@+id/extend_warraty"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="تمديد مدة التعميد"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toStartOf="@id/make_payment"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintHorizontal_chainStyle="spread"
                        app:layout_constraintStart_toStartOf="parent" />

                    <Button
                        android:id="@+id/make_payment"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="8dp"
                        android:background="@mipmap/bt_grey1"
                        android:text="الدفع"
                        android:textColor="@color/white"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/extend_warraty" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>